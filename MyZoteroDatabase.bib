}

@incollection{helgaker_multiconfigurational_2014,
	title = {Multiconfigurational {Self}‐{Consistent} {Field} {Theory}},
	isbn = {978-1-118-53147-1},
	abstract = {This chapter introduces the tools needed for optimization and manipulation of the multiconfigurational self-consistent field (MCSCF) wave function. It treats two different but related methods for the optimization of the MCSCF wave function. The first method is a straightforward application of Newton's method for the optimization of nonlinear functions, modified so as to ensure global convergence. The second method is based on the solution of an eigenvalue equation which reduces to the standard eigenvalue problem of CI theory when no orbital optimization is included. The chapter discusses how these linear equations are solved for large MCSCF expansions, and introduces an exponential unitary parametrization of the configuration space. The triplet and singlet instabilities occur when RHF wave functions are used to describe the breaking of chemical bonds.},
