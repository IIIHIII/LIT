	file = {Full Text PDF:C\:\\Users\\Herbert.DESKTOP-L7DEVQ5\\Zotero\\storage\\GSKEHTF8\\Schinabeck et al. - 2019 - A symmetry-based design strategy for unprecedented.pdf:application/pdf},
}

