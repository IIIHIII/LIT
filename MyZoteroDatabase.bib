@article{chen_spectrum_2018-1,
	title = {Spectrum {Features} {Analysis} of {Ocean} {Wind} and {Wave} {Based} on {Fast} {Fourier} {Transform} {Algorithm}},
	issn = {0749-0208},
	url = {https://www.jstor.org/stable/26542994},
