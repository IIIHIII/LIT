@article{artemev_chemoselective_2019,
	title = {Chemoselective mechanochemical route toward a bright {TADF}-emitting {CuI}–based coordination polymer},
	volume = {6},
	doi = {10.1039/C8QI01302K},
	abstract = {An efficient solvent-assistant mechanochemical synthesis of a bright TADF-emitting CuI–based compound that is inaccessible by conventional wet methods, is reported for the first time. The simple manual grinding of 2-(ethylsulfanyl)-6-methylpyridine (L) with CuI in the presence of a few drops of MeCN for 1–2 min chemoselectively gives the 1-D coordination polymer (CP), [Cu3I3L2]n (CP1) as a single product isolated in 87\% yield. Under solvent-free conditions or in solution, this reaction results in non-emissive complex, [Cu2I2L2] (C1) or structurally different 1-D CP, [Cu3I3L2]n (CP3), respectively. Remarkably, both C1 and CP3 under the mechanochemical conditions easily (1–2 min) undergo polymerization or isomerisation, accordingly, to afford CP1. The structure of the latter is formed by cuprophilic interactions-free (–Cu–I–)n chains ligated by 2-(ethylsulfanyl)-6-methylpyridines in a μ2-N,S manner. At room temperature, CP1 exhibits highly efficient blue-green TADF with short lifetime (4.0 μs) and high photoluminescence performance (PLQY being 70\%).},
	journal = {Inorganic Chemistry Frontiers},
	author = {Artem’ev, Alexander and Doronina, Evgeniya and Rakhmanova, Mariana and Tarasova, Olga and Bagryanskaya, Irina and Nedolya, Nina},
	month = jan,
	year = {2019},
	pages = {671--679},
	file = {Full Text PDF:C\:\\Users\\Herbert.DESKTOP-L7DEVQ5\\Zotero\\storage\\TAK9ER6T\\Artem’ev et al. - 2019 - Chemoselective mechanochemical route toward a brig.pdf:application/pdf},
}

