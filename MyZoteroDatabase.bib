	file = {arXiv Fulltext PDF:C\:\\Users\\Herbert.DESKTOP-L7DEVQ5\\Zotero\\storage\\4W48PMEI\\Homeier - 1996 - Correlation Energy Estimators based on M o ller-P.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\Herbert.DESKTOP-L7DEVQ5\\Zotero\\storage\\NFAZNRNY\\9603001.html:text/html},
