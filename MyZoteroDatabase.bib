@article{colombier_detection_2012,
	title = {Detection of a {Biexciton} in {Semiconducting} {Carbon} {Nanotubes} {Using} {Nonlinear} {Optical} {Spectroscopy}},
	volume = {109},
	doi = {10.1103/PhysRevLett.109.197402},
	abstract = {We report the observation of the biexciton in semiconducting single-wall carbon nanotubes by means of nonlinear optical spectroscopy. Our measurements reveal the universal asymmetric line shape of the Fano resonance intrinsic to the biexciton transition. For nanotubes of the (9,7) chirality, we find a biexciton binding energy of 106 meV. From the calculation of the χ{\textasciicircum}\{(3)\} nonlinear response, we provide a quantitative interpretation of our measurements, leading to an estimation of the characteristic Fano factor q of 7±3. This value allows us to extract the first experimental information on the biexciton stability and we obtain a biexciton annihilation rate comparable to the exciton-exciton annihilation one.},
	journal = {Physical review letters},
	author = {Colombier, Leo and Selles, Julien and Rousseau, Emmanuel and Lauret, Jean-Sébastien and Vialla, Fabien and Voisin, Christophe and Cassabois, Guillaume},
	month = nov,
	year = {2012},
	pages = {197402},
	file = {Full Text PDF:C\:\\Users\\Herbert.DESKTOP-L7DEVQ5\\Zotero\\storage\\TH5VH9QZ\\Colombier et al. - 2012 - Detection of a Biexciton in Semiconducting Carbon .pdf:application/pdf},
}

@article{eckstein_quantifying_2019,
	title = {Quantifying {Doping} {Levels} in {Carbon} {Nanotubes} by {Optical} {Spectroscopy}},
	volume = {XXXX},
	doi = {10.1021/acs.jpcc.9b08663},
	abstract = {Controlling doping is essential for a successful integration of semiconductor materials into device technologies. However, the assessment of doping levels and the distribution of charge carriers in carbon nanotubes or other nanoscale semiconductor materials is often either limited to a qualitative attribution of being 'high' or 'low' or it is entirely absent. Here, we describe efforts toward a quantitative characterization of doping in redox- or electrochemically doped semiconducting carbon nanotubes (s-SWNTs) using VIS-NIR absorption spectroscopy. We discuss how carrier densities up to about 0.5 nm⁻¹ can be quantified with a sensitivity of roughly one charge per 10⁴ carbon atoms assuming in-homogeneous or homogeneous carrier distributions.},
	journal = {The Journal of Physical Chemistry C},
	author = {Eckstein, Klaus and Oberndorfer, Florian and Achsnich, Melanie and Schöppler, Friedrich and Hertel, Tobias},
	month = nov,
	year = {2019},
	file = {Submitted Version:C\:\\Users\\Herbert.DESKTOP-L7DEVQ5\\Zotero\\storage\\2TFFNKPU\\Eckstein et al. - 2019 - Quantifying Doping Levels in Carbon Nanotubes by O.pdf:application/pdf},
}

@book{kuang_charge_2021,
	title = {Charge {Transfer} from {Photoexcited} {Semiconducting} {Single}-{Walled} {Carbon} {Nanotubes} to {Wide}-{Bandgap} {Wrapping} {Polymer}},
	abstract = {As narrow optical bandgap materials, semiconducting single-walled carbon nanotubes (SWCNTs) are rarely regarded as charge donors in photoinduced charge-transfer (PCT) reactions. However, the unique band structure and unusual exciton dynamics of SWCNTs add more possibilities to the classical PCT mechanism.In this work, we demonstrate PCT from photoexcited semiconducting (6,5) SWCNTs to a wide-bandgap wrapping poly-[(9,9-dioctylfluorenyl-2,7-diyl)-alt-(6,6')-(2,2'-bipyridine)] (PFO-BPy) via femtosecond transient absorption spectroscopy. By monitoring the spectral dynamics of the SWCNT polaron, we show that charge transfer from photoexcited SWCNTs to PFO-BPy can be driven not only by the energetically favorable E33 transition but also by the energetically unfavorable E22 excitation under high pump fluence. This unusual PCT from narrow-bandgap SWCNTs toward a wide-bandgap polymer originates from the up-converted high-energy excitonic state (E33 or higher) that is promoted by the Auger recombination of excitons and charge carriers in SWCNTs. These insights provide new pathways for charge separation in SWCNT-based photodetectors and photovoltaic cells.},
	author = {Kuang, Zhuoran and Berger, Felix and Lustres, Jose and Wollscheid, Nikolaus and Li, Han and Lüttgens, Jan and Leinen, Merve and Flavel, Benjamin and Zaumseil, Jana and Buckup, Tiago},
	month = may,
	year = {2021},
	file = {Full Text PDF:C\:\\Users\\Herbert.DESKTOP-L7DEVQ5\\Zotero\\storage\\5E2WI8YH\\Kuang et al. - 2021 - Charge Transfer from Photoexcited Semiconducting S.pdf:application/pdf},
}

@article{kahmann_excited_2017,
	title = {Excited {State} {Interaction} of {Semiconducting} {Single} {Walled} {Carbon} {Nanotubes} with {Their} {Wrapping} {Polymers}},
	volume = {8},
	doi = {10.1021/acs.jpclett.7b02553},
	abstract = {Excited state dynamics and energy levels of semiconducting single walled carbon nanotubes (CNT) wrapped with conjugated polymers are commonly discussed as if the polymer chains were absent. Similarly, when employed in charge carrying devices, e.g. field effect transistors, the polymer is at maximum considered as a potential barrier for inter-tube charge carrier hopping. Here, we employ photoluminescence- and pump-probe spectroscopy on films of semiconducting CNTs of different chirality wrapped with either a wide band gap polyfuorene derivative (PF12) or a polythiophene with narrower gap (P3DDT) to elucidate the excited states' interplay between the two materials in thin films. Excitation above the polymer band gap gives way to an ultrafast electron transfer from both polymers towards the CNTs. By monitoring the hole polaron on the polymer via its mid infrared signature, we show that also illumination below the polymer band gap leads to the formation of this fingerprint, and infer that holes are also transferred towards the polymer. As this contradicts the standard way of discussing the involved energy levels, we propose that polymer wrapped CNTs should be considered as a single hybrid system, exhibiting states shared between the two components. This proposition is validated through quantum chemical calculations that show hybridization of the first excited states, especially for the thiohpene-CNT sample.},
	journal = {The Journal of Physical Chemistry Letters},
	author = {Kahmann, Simon and Salazar Rios, Jorge and Zink, Matthias and Allard, Sybille and Scherf, Ullrich and Santos, Maria and Brabec, Christoph and Loi, Maria},
	month = nov,
	year = {2017},
	file = {Full Text PDF:C\:\\Users\\Herbert.DESKTOP-L7DEVQ5\\Zotero\\storage\\B9BNPWZB\\Kahmann et al. - 2017 - Excited State Interaction of Semiconducting Single.pdf:application/pdf},
}

@article{scuratti_charge_2021,
	title = {Charge {Transport} in {High}‐{Mobility} {Field}‐{Effect} {Transistors} {Based} on {Inkjet} {Printed} {Random} {Networks} of {Polymer} {Wrapped} {Single}‐{Walled} {Carbon} {Nanotubes}},
	volume = {31},
	doi = {10.1002/adfm.202006895},
	abstract = {Printed random networks of polymer‐wrapped multi‐chiral semiconducting carbon nanotubes (s‐SWCNTs) are an opportunity for mass‐manufacturable, high‐performance large‐area electronics. To meet this goal, a deeper understanding of charge‐transport mechanisms in such mixed networks is crucial. Here, charge transport in field‐effect transistors based on inkjet‐printed s‐SWCNTs networks is investigated, obtaining direct evidence for the phases probed by charge in the accumulated channel, which is critical information to rationalize the different transport properties obtained for different printing conditions. In particular, when the fraction of nanotubes with smaller bandgaps is efficiently interconnected, the sparse network provides efficient charge percolation for band‐like transport, with a charge mobility as high as 20.2 cm² V⁻¹ s⁻¹. However, when the charges are forced by a less efficient morphology, to populate also higher bandgap nanotubes and and/or the wrapping polymer, thermally activated transport takes place and mobility drops. As a result, a trade‐off between network density and charge transport properties is identified for device current optimization, in both p‐ and n‐type regimes. These findings shed light on the fundamental aspects related to charge transport in printed s‐SWCNT mixed networks and contribute to devise appropriate strategies for the formulation of inks and processes towards cost‐effective mass production schemes of high‐performance large‐area electronics.},
	journal = {Advanced Functional Materials},
	author = {Scuratti, Francesca and Salazar Rios, Jorge and Luzio, Alessandro and Kowalski, Sebastian and Allard, Sybille and Jung, Stefan and Scherf, Ullrich and Loi, Maria and Caironi, Mario},
	month = jan,
	year = {2021},
	pages = {2006895},
	file = {Full Text PDF:C\:\\Users\\Herbert.DESKTOP-L7DEVQ5\\Zotero\\storage\\TJCWFNYX\\Scuratti et al. - 2021 - Charge Transport in High‐Mobility Field‐Effect Tra.pdf:application/pdf},
}

@article{junxing_photosensitive-stamp-inspired_2022,
	title = {Photosensitive-{Stamp}-{Inspired} {Scalable} {Fabrication} {Strategy} of {Wearable} {Sensing} {Arrays} for {Noninvasive} {Real}-{Time} {Sweat} {Analysis}},
	doi = {10.1021/acs.analchem.2c00593},
	abstract = {Wearable sweat sensing is essential to the development of personalized health monitoring in a noninvasive manner with molecular-level insight. Hence, there is an increasing demand for convenient, facile, and efficient fabrication of wearable sensing arrays. Inspired by a photosensitive stamp (PS), we present herein a simple, low-cost, and eco-friendly vacuum filtration-transfer printing method (termed PS-VFTP) for the scalable preparation of single-walled carbon nanotube (SWCNT) based flexible electrode arrays. This method can economically yield customized flexible SWCNT arrays with praiseworthy performance, such as high reproducibility, precision, uniformity, conductivity, and mechanical stability. In addition, the flexible SWCNT arrays can be easily functionalized into high-performance electrochemical sensors for the simultaneous monitoring of sweat metabolites (glucose, lactate) and electrolytes (Na+, K+). The integration of wearable sensing arrays with a signal acquisition and processing circuit system in the intelligent wearable sensors empowers them to realize noninvasive, real-time, and in situ sweat analysis during exercise. More meaningfully, such a PS-VFTP strategy can be easily expanded to the economical manufacturing of other flexible electronic devices.},
	journal = {Analytical Chemistry},
	author = {Junxing, Hao and Zhu, Zeqiang and Hu, Chengguo and Liu, Zhihong},
	month = mar,
	year = {2022},
}

