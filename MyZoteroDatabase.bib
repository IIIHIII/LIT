@article{petkovic_general_2010-3,
	title = {{ON} {A} {GENERAL} {CLASS} {OF} {MULTIPOINT} {ROOT}-{FINDING} {METHODS} {OF} {HIGH} {COMPUTATIONAL} {EFFICIENCY}},
	volume = {47},
