@article{weber_higher_2004,
	title = {Higher {Order} {Response} in \$\{{\textbackslash}cal {O}\}({N})\$ by {Perturbed} {Projection}},
	abstract = {Perturbed projection for linear scaling solution of the coupled-perturbed self-consistent-field equations [Weber, Niklasson and Challacombe, Phys. Rev. Lett. 92, 193002 (2004)] is extended to the computation of higher order static response properties. Although generally applicable, perturbed projection is developed here in the context of the self-consistent first and second electric hyperpolar-izabilities of three dimensional water clusters at the Hartree-Fock level of theory. Non-orthogonal, density matrix analogues of Wigner's 2n + 1 rule are given up to fourth order. Linear scaling and locality of the higher order response densities under perturbation by a global electric field are demonstrated.},
