@article{shou_three_2017,
	title = {Three {Luminescent} {Copper}({I}) {Iodide} {Clusters} with {Phosphine} {Ligands}: {Synthesis}, {Structure} {Characterization}, {Properties} and {TD}-{DFT} {Calculations}},
