@article{shan_polydopamine-modified_2020,
	title = {Polydopamine-{Modified} {Metal}-{Organic} {Frameworks}, {NH2}-{Fe}-{MIL}-101, as {pH}-{Sensitive} {Nanocarriers} for {Controlled} {Pesticide} {Release}},
	volume = {10},
	doi = {10.3390/nano10102000},
	abstract = {Recently, metal-organic frameworks (MOFs) have become a dazzling star among porous materials used in many fields. Considering their intriguing features, MOFs have great prospects for application in the field of sustainable agriculture, especially as versatile pesticide-delivery vehicles. However, the study of MOF-based platforms for controlled pesticide release has just begun. Controlled pesticide release responsive to environmental stimuli is highly desirable for decreased agrochemical input, improved control efficacy and diminished adverse effects. In this work, simple, octahedral, iron-based MOFs (NH2-Fe-MIL-101) were synthesized through a microwave-assisted solvothermal method using Fe3+ as the node and 2-aminoterephthalic acid as the organic ligand. Diniconazole (Dini), as a model fungicide, was loaded into NH2-Fe-MIL-101 to afford Dini@NH2-Fe-MIL-101 with a satisfactory loading content of 28.1\%. The subsequent polydopamine (PDA) modification could endow Dini with pH-sensitive release patterns. The release of Dini from PDA@Dini@NH2-Fe-MIL-101 was much faster in an acidic medium compared to that in neutral and basic media. Moreover, Dini@NH2-Fe-MIL-101 and PDA@Dini@NH2-Fe-MIL-101 displayed good bioactivities against the pathogenic fungus causing wheat head scab (Fusarium graminearum). This research sought to reveal the feasibility of versatile MOFs as a pesticide-delivery platform in sustainable crop protection.},
	journal = {Nanomaterials (Basel, Switzerland)},
	author = {Shan, Yongpan and Xu, Chunli and Zhang, Hongjun and Chen, Huiping and Bilal, Muhammad and Niu, Shujun and Cao, Lidong and ql, Huqng},
	month = oct,
	year = {2020},
}

@techreport{homeier_convergence_1997,
	address = {Regensburg},
	type = {Technical {Report} and preprint},
	title = {Convergence {Acceleration} of {Logarithmically} {Convergent} {Series} {Avoiding} {Summation}},
	url = {https://epub.uni-regensburg.de/19812/1/Convergence-acceleration-of-logarithmically-convergent-series-avoiding-summation_TC-NA-97-8.pdf},
	abstract = {Quite often in application, logarithmically convergent series have to be evaluated. There are several convergence acceleration methods that are based on the evaluation of partial sums sn for relatively large n, and thus, normally require the evaluation of all terms aj with 0 ≤ j ≤ n. Here, we show that it is possible to avoid the computation of the partials sums of high order if it is possible to evaluate a few terms aj for relatively large j. The effectiveness of the approach is demonstrated for the 1/z expansion that is a particularly difficult example of logarithmic convergence.},
	number = {TC-NA-97-8},
	urldate = {2020-12-06},
