title={The strange history of B functions or how theoretical chemists and mathematicians do (not) interact},
journal={International Journal of Quantum Chemistry},
volume={109},
number={8},
publisher={Wiley Subscription Services, Inc., A Wiley Company},
issn={1097-461X},
url={http://dx.doi.org/10.1002/qua.22014},
