@article{Shakhno2009222,
title="On an iterative algorithm with superquadratic convergence for solving nonlinear operator equations",
journal="Journal of Computational and Applied Mathematics",
volume="231",
number="1",
pages="222 - 235",
year="2009",
note="",
issn="0377-0427",
doi="http://dx.doi.org/10.1016/j.cam.2009.02.010",
url="http://www.sciencedirect.com/science/article/pii/S0377042709000569",
author="S.M. Shakhno",
keywords="Iterative difference method",
keywords="Divided differences",
keywords="Convergence order",
keywords="Majorant method",
keywords="Lipschitz–Hölder condition",
abstract="We study an iterative method with order ( 1 + 2 ) for solving nonlinear operator equations in Banach spaces. Algorithms for specific operator equations are built up. We present the received new results of the local and semilocal convergence, in case when the first-order divided differences of a nonlinear operator are Hölder continuous. Moreover a quadratic nonlinear majorant for a nonlinear operator, according to the conditions laid upon it, is built. A priori and a posteriori estimations of the method’s error are received. The method needs almost the same number of computations as the classical Secant method, but has a higher order of convergence. We apply our results to the numerical solving of a nonlinear boundary value problem of second-order and to the systems of nonlinear equations of large dimension.",
}

