@article {Weniger02ata,
author={Weniger, Ernst Joachim},
title={Addition theorems as three-dimensional Taylor expansions. II. B functions and other exponentially decaying functions},
journal={International Journal of Quantum Chemistry},
volume={90},
number={1},
publisher={Wiley Subscription Services, Inc., A Wiley Company},
issn={1097-461X},
url={http://dx.doi.org/10.1002/qua.948},
doi={10.1002/qua.948},
pages={92--104},
keywords={addition theorems, Taylor expansions, spherical tensor gradient operator, B functions, other exponentially decaying functions},
year={2002},
abstract={Addition theorems can be constructed by doing three-dimensional Taylor expansions according to f(r+râ€²)=expâ€‰(râ€²â‹…âˆ‡)f(r). Since, however, one is normally interested in addition theorems of irreducible spherical tensors, the application of the translation operator in its Cartesian form expâ€‰(xâ€²âˆ‚/âˆ‚x)expâ€‰(yâ€²âˆ‚/âˆ‚y)expâ€‰(zâ€²âˆ‚/âˆ‚z) would lead to enormous technical problems. A better alternative consists in using a series expansion for the translation operator expâ€‰(râ€²â‹…âˆ‡) involving powers of the Laplacian âˆ‡2 and spherical tensor gradient operators \documentclass{article}\pagestyle{empty}\begin{document}$\mathcal{Y}^{m}_{\ell}(\nabla)$\end{document}, which are irreducible spherical tensors of ranks zero and â„“, respectively (Santos, F. D. Nucl Phys A 1973, 212, 341). In this way, it is indeed possible to derive addition theorems by doing three-dimensional Taylor expansions (Weniger, E. J. Int J Quantum Chem 2000, 76, 280). The application of the translation operator in its spherical form is particularly simple in the case of B functions and leads to an addition theorem with a comparatively compact structure. Since other exponentially decaying functions like {Slater}-type functions, bound-state hydrogenic eigenfunctions, and other functions based on generalized Laguerre polynomials can be expressed by simple finite sums of B functions, the addition theorems for these functions can be written down immediately. Â© 2002 Wiley Periodicals, Inc. Int J Quantum Chem, 2002},
}


